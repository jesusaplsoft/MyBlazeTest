<project xmlns="http://maven.apache.org/POM/4.0.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>MyBlazeTest</groupId>
  <artifactId>MyBlazeTest</artifactId>
  <version>0.0.1-SNAPSHOT</version>
  <name>BlazeProject2MakeTests</name>
  <description>Blaze Project to make tests</description>
  <build>
    <resources>
      <resource>
        <targetPath>./</targetPath>
        <directory>src/main/resources</directory>
        <includes>
          <include>bundle/*.properties</include>
          <include>dbUpdate/**/*.sql</include>
          <include>images/**/*.*</include>
          <include>sun/**/*.*</include>
          <include>img/*.*</include>
          <include>img/**/*.png</include>
          <include>db/migration/*.sql</include>
          <include>jasper/**/*.jasper</include>
          <include>jfd/**/*.jfd</include>
          <include>jasperreports.properties</include>
          <include>search/stop_es.txt</include>
          <include>lombok.config</include>
          <include>log4j2.xml</include>
          <include>db.properties</include>
          <include>db_jpa.properties</include>
          <include>dictionaries/*</include>
          <include>dataSourceContext.xml</include>
          <include>client_secrets.json</include>
        </includes>
      </resource>
      <resource>
        <targetPath>./</targetPath>
        <directory>src/main/java</directory>
        <excludes>
          <exclude>com/**/*</exclude>
          <exclude>db/**/*</exclude>
        </excludes>
      </resource>
      <resource>
        <targetPath>./search</targetPath>
        <directory>src/main/resources/search</directory>
        <includes>
          <include>stop_es.txt</include>
        </includes>
      </resource>
      <resource>
        <targetPath>./etc/html</targetPath>
        <directory>src/main/resources/html</directory>
        <includes>
          <include>css/**</include>
          <include>images/**</include>
          <include>mail.html</include>
          <include>attached.html</include>
        </includes>
      </resource>
    </resources>
    <testResources>
      <testResource>
        <targetPath>./help</targetPath>
        <directory>doc/help</directory>
      </testResource>
      <testResource>
        <targetPath>./</targetPath>
        <directory>src/main/resources</directory>
        <!-- NO PONER LA BASE DE DATOS AQUÍ -->
        <includes>
          <include>bundle/*.properties</include>
          <include>img/*.*</include>
          <include>img/**/*.png</include>
          <include>jasper/**/*.jasper</include>
          <include>jfd/**/*.jfd</include>
          <include>jasperreports.properties</include>
          <include>db_jpa.properties</include>
        </includes>
      </testResource>
      <testResource>
        <targetPath>./</targetPath>
        <directory>src/test/resources</directory>
        <includes>
          <include>log4j2.xml</include>
          <include>db_jpa-test.properties</include>
          <include>db.properties</include>
          <include>preguntas/*.*</include>
          <include>client_secrets.json</include>
        </includes>
      </testResource>
    </testResources>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>3.8.1</version>
        <configuration>
          <release>17</release>
        </configuration>
      </plugin>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-jar-plugin</artifactId>
        <version>3.3.0</version>
        <configuration>
          <archive>
            <addMavenDescriptor>false</addMavenDescriptor>
            <index>true</index>
            <manifest>
              <mainClass>com.gexcat.gex.main.GexCat</mainClass>
            </manifest>
            <manifestEntries>
              <nombre>${project.artifactId}</nombre>
              <version>${project.version}</version>
              <Built-By>${user.name}</Built-By>
              <Build-Timestamp>${timestamp}</Build-Timestamp>
              <Build-Revision>${version.number}</Build-Revision>
              <Created-By>Apache Maven ${maven.version}</Created-By>
              <Build-Jdk>${java.version} ${java.vendor}
                ${java.vm.version}</Build-Jdk>
              <Build-OS>${os.name} ${os.arch} ${os.version}</Build-OS>
              <Implementation-Version>${project.version}</Implementation-Version>
            </manifestEntries>
          </archive>
          <excludes>
            <exclude>**/pom.xml</exclude>
            <exclude>META-INF/maven/**</exclude>
          </excludes>
          <finalName>${project.build.finalName}-big</finalName>
        </configuration>
      </plugin>
    </plugins>
  </build>
